[
    { 
        "key": "cmd+left",              
        "command": "-cursorHome",
        "when": "editorTextFocus" 
    },
    { 
        "key": "cmd+right",             
        "command": "-cursorEnd",
        "when": "editorTextFocus" 
    },
    {
        "key": "cmd+left",
        "command": "cursorLineStart",
        "when": "editorTextFocus"
    },
    {
        "key": "cmd+right",
        "command": "cursorLineEnd",
        "when": "editorTextFocus"
    }
]